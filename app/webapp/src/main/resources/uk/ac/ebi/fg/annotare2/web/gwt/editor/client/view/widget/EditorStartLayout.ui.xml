<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >
    <g:DockLayoutPanel>
        <g:north size="25">
            <g:SimpleLayoutPanel ui:field="topBarDisplay" height="100%"/>
        </g:north>
        <g:center>
            <g:SimpleLayoutPanel ui:field="display"/>
        </g:center>
        <g:south size="20">
            <g:HTMLPanel>
                <div class="copy">2020 EMBL-EBI, Molecular Atlas Group</div>
            </g:HTMLPanel>
        </g:south>
    </g:DockLayoutPanel>
</ui:UiBinder>
