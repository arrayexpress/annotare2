<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:DockPanel width="100%">
        <g:Dock direction="NORTH" height="60px">
            <g:SimplePanel ui:field="topPanel" />
        </g:Dock>
        <g:Dock direction="WEST" width="200px">
            <g:SimplePanel ui:field="westPanel"/>
        </g:Dock>
        <g:Dock direction="CENTER">
            <g:SimplePanel ui:field="centerPanel"/>
        </g:Dock>
        <g:Dock direction="SOUTH" height="30px">
            <g:HTMLPanel>
                <div class="copy"> 2012 EMBL-EBI, Funcitonal Genomics Group </div>
            </g:HTMLPanel>
        </g:Dock>
    </g:DockPanel>

</ui:UiBinder>

