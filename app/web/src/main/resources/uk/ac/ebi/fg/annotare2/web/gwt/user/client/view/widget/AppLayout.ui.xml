<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:DockPanel width="100%">
        <g:Dock direction="NORTH">
            <g:SimplePanel ui:field="topPanel"/>
        </g:Dock>
        <g:Dock direction="CENTER">
            <g:HTMLPanel width="100%">
                <table cellpadding="0" cellspacing="0" width="100%">
                    <tr>
                        <td width="200px" class="app-MenuContent">
                            <g:SimplePanel ui:field="westPanel"/>
                        </td>
                        <td class="app-ListContent">
                            <g:SimplePanel ui:field="centerPanel"/>
                        </td>
                    </tr>
                </table>
            </g:HTMLPanel>
        </g:Dock>
        <g:Dock direction="SOUTH">
            <g:HTML>
                <div class="copy"> 2012 EMBL-EBI, Functional Genomics Group </div>
            </g:HTML>
        </g:Dock>
    </g:DockPanel>

</ui:UiBinder>

