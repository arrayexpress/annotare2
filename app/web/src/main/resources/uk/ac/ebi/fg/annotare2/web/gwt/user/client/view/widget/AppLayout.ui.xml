<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <g:DockLayoutPanel unit="EM" width="100%" height="100%">
        <g:north size="9">
            <g:SimplePanel ui:field="topPanel"/>
        </g:north>
        <g:center>
            <g:DockLayoutPanel unit="PX" width="100%" height="100%">
                <g:west size="200">
                    <g:SimplePanel ui:field="westPanel"/>
                </g:west>
                <g:center>
                    <g:SimpleLayoutPanel ui:field="centerPanel"/>
                </g:center>
            </g:DockLayoutPanel>
        </g:center>
        <g:south size="2">
            <g:HTML>
                <div class="copy">2014 EMBL-EBI, Functional Genomics Group</div>
            </g:HTML>
        </g:south>
    </g:DockLayoutPanel>

</ui:UiBinder>

