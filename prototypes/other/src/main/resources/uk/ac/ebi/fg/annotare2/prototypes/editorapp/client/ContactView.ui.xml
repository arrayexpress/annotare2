<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        xmlns:wgt="urn:import:uk.ac.ebi.fg.annotare2.prototypes.editorapp.client">

    <g:DisclosurePanel ui:field="disclosurePanel" width="100%">
        <g:header>Header</g:header>
        <g:HTMLPanel>
            <table>
                <tr>
                    <td>Name:</td>
                    <td><wgt:TextBoxWithPlaceHolder placeHolder="Last Name"/></td>
                    <td><g:TextBox/></td>
                    <td><g:TextBox/></td>
                </tr>
                <tr>
                    <td>Address:</td>
                    <td><g:TextBox/></td>
                </tr>
            </table>
        </g:HTMLPanel>
    </g:DisclosurePanel>
</ui:UiBinder>