<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui"
        >
    <ui:style>
        .padding {
            margin-left: 5px;
        }
    </ui:style>
    <g:DockLayoutPanel addStyleNames="{style.padding}">
        <g:north size="60">
            <g:HTMLPanel>
                <h2><g:Label ui:field="title"/></h2>
            </g:HTMLPanel>
        </g:north>
        <g:center>
            <g:DockLayoutPanel ui:field="tablePanel"/>
        </g:center>
    </g:DockLayoutPanel>
</ui:UiBinder>