<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
        xmlns:ui="urn:ui:com.google.gwt.uibinder"
        xmlns:g="urn:import:com.google.gwt.user.client.ui">

    <ui:with field="res" type="uk.ac.ebi.fg.annotare2.prototypes.editorapp.client.EditorClientBundle"/>

    <g:HTMLPanel>
        <table width="100%">
            <tbody>
                <tr>
                    <td width="20px"><span class="{res.moreStyles.draggableIconClass}"></span></td>
                    <td width="20px"><span class="{res.moreStyles.triangleIconClass}"></span></td>
                    <td><g:Label ui:field="label"/></td>
                    <td></td>
                    <td width="20px">
                        <g:CheckBox></g:CheckBox>
                    </td>
                </tr>
            </tbody>
        </table>
    </g:HTMLPanel>
</ui:UiBinder>